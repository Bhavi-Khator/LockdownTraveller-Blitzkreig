<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="DisplayTrainsController">
    <children>
        <Label layoutX="217.0" layoutY="5.0" prefHeight="35.0" prefWidth="147.0" text="       Find Trains" textOverrun="CENTER_ELLIPSIS">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <TextField fx:id="sourceTextField" layoutX="216.0" layoutY="90.0" promptText="Enter Source" />
        <TextField fx:id="destTextField" layoutX="216.0" layoutY="139.0" promptText="Enter Destination" />
        <DatePicker fx:id="dateDatePicker" layoutX="216.0" layoutY="187.0" prefHeight="25.0" prefWidth="147.0" promptText="dd/mm/yyyy" />
        <Button fx:id="Display" layoutX="216.0" layoutY="278.0" mnemonicParsing="false" onAction="#displayTrains" prefHeight="25.0" prefWidth="149.0" text="Display Trains" />
    </children>
</AnchorPane>
